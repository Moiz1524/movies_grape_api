<p>New Movie</p>

<%= form_with(model: @movie, local: true) do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :released %>
  <%= f.datetime_field :released %>

  <%= f.label :points %>
  <%= f.number_field :points %>

  <%= f.label :liked %>
  <%= f.check_box :liked %>

<% end %>
